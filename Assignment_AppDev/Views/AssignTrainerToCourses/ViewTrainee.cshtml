@model IEnumerable<Assignment_AppDev.Models.ApplicationUser>
@{
    ViewBag.Title = "ViewTrainee";
}

<h2>ViewTrainee</h2>
<table class="table">
    <thead>
        <tr>

            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>
</table>


